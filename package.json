{
  "name": "satellite-house",
  "version": "1.0.0",
  "description": "Website for Satellite House design",
  "main": "src/index.js",
  "repository": "git@github.com:jdobrowner/satellite-house.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start:dev-server",
    "start:dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:js": "webpack",
    "build:css": "rm -f build/style.css && lessc style/style.less --clean-css dist/style.css",
    "build:html": "rm -f dist/index.html && cp index.html dist/",
    "build:images": "cp -r images/* dist/",
    "build": "rm -rf dist && mkdir -p dist && npm run build:html && npm run build:css && npm run build:js",
    "watch:css": "npm run build:css && chokidar-cmd -c 'npm run build:css' -t style/**/*.less",
    "watch": "webpack --watch & npm run watch:css"
  },
  "author": "Jason Dobrowner",
  "license": "ISC",
  "dependencies": {
    "nuka-carousel": "^2.0.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "chokidar": "^1.6.1",
    "chokidar-cmd": "^1.2.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.2",
    "uglify-js": "^2.8.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
